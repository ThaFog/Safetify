{
  "name": "safetify",
  "version": "2.3.0",
  "description": "Var and property safing utility",
  "main": "dist/safetify.js",
  "scripts": {
    "build": "webpack",
    "test-js": "webpack & node spec/config/jasmine.js",
    "test": "webpack & jasmine-ts --config=spec/config/jasmine.json spec/ts/**/*.spec.ts",
    "jsdoc": "typedoc src/safetify.ts --mode file --out docs/ --theme typedoc-theme"
  },
  "keywords": [
    "var",
    "val",
    "variable",
    "value",
    "property",
    "json",
    "resolver",
    "resolve",
    "decoder",
    "decode",
    "safing",
    "safety",
    "safe"
  ],
  "author": {
    "name": "Dominik Mielcarek",
    "email": "fogbpl@gmail.com",
    "url": "https://github.com/ThaFog"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "jasmine": "^2.9.0",
    "jasmine-core": "^2.9.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "ts-loader": "^3.4.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThaFog/Safetify.git"
  },
  "homepage": "https://thafog.github.io/Safetify/index.html",
  "directories": {
    "lib": "./src",
    "test": "./spec"
  },
  "bugs": {
    "url": "https://github.com/ThaFog/Safetify/issues"
  },
  "files": [
    "dist/safetify.js",
    "index.d.ts",
    "LICENSE"
  ]
}
