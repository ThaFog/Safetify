{
  "name": "safetify",
  "version": "3.0.1",
  "description": "Utility for resolving and making values types safe",
  "main": "dist/safetify.js",
  "scripts": {
    "build": "webpack",
    "test": "webpack & jasmine-ts --config=spec/config/jasmine.json spec/**/*.spec.ts",
    "jsdoc": "typedoc src/safetify.ts --mode file --out docs/ --theme typedoc-theme",
    "jsdoc:noerrors": "npm run jsdoc -- --ignoreCompilerErrors",
    "lint": "tslint src/**/*.ts  -p tsconfig.json"
  },
  "keywords": [
    "var",
    "val",
    "variable",
    "value",
    "property",
    "json",
    "resolver",
    "resolve",
    "decoder",
    "decode",
    "safing",
    "safety",
    "safe",
    "type",
    "types",
    "check",
    "checking",
    "type-check",
    "type-checking",
    "parameters",
    "data",
    "validation",
    "validate",
    "assert",
    "verify"
  ],
  "author": {
    "name": "Dominik Mielcarek",
    "email": "fogbpl@gmail.com",
    "url": "https://github.com/ThaFog"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "jasmine": "^2.9.0",
    "jasmine-core": "^2.9.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.11.1",
    "typescript": "^3.1.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.6.0"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThaFog/Safetify.git"
  },
  "homepage": "https://thafog.github.io/Safetify/index.html",
  "directories": {
    "lib": "./src",
    "test": "./spec"
  },
  "bugs": {
    "url": "https://github.com/ThaFog/Safetify/issues"
  },
  "files": [
    "dist/safetify.js",
    "index.d.ts",
    "LICENSE"
  ]
}
